{
    "version": 2,
    "name": "QUIZ_game_app",
    "framework": "python",
    "rootDirectory": "./",
    "builds": [
      {
        "src": "(app3.py)",
        "use": "@vercel/python-build",
        "config": {
          "requirements": "requirements.txt"
        }
      }
    ],
    "routes": [
      {
        "src": "/*",
        "dest": "(app3.py)"
      }
    ]
  }
  